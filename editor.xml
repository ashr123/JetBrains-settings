<application>
  <component name="CodeInsightSettings">
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="HIGHLIGHT_SCOPE" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="REMOVE_TRAILING_BLANK_LINES" value="true" />
    <option name="ENABLE_RENDERED_DOC" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="microservices.url.path.inlay.hints" />
    <settingsMapElement>
      <JavaScript.js.chain.hints>
        <Settings>
          <option name="uniqueTypeCount" value="1" />
        </Settings>
      </JavaScript.js.chain.hints>
      <JavaScript.js.type.hints>
        <Settings>
          <option name="showForFunctions" value="true" />
          <option name="showForShorthandFunctions" value="true" />
          <option name="showForVariablesAndFields" value="true" />
        </Settings>
      </JavaScript.js.type.hints>
      <TypeScript.ts.chain.hints>
        <Settings>
          <option name="uniqueTypeCount" value="1" />
        </Settings>
      </TypeScript.ts.chain.hints>
      <TypeScript.ts.type.hints>
        <Settings>
          <option name="showForFunctions" value="true" />
          <option name="showForShorthandFunctions" value="true" />
          <option name="showForVariablesAndFields" value="true" />
        </Settings>
      </TypeScript.ts.type.hints>
      <JAVA.chain.hints>
        <Settings>
          <option name="uniqueTypeCount" value="1" />
        </Settings>
      </JAVA.chain.hints>
      <JAVA.annotation.hints>
        <Settings>
          <option name="showInferred" value="true" />
        </Settings>
      </JAVA.annotation.hints>
      <JAVA.JavaLens>
        <JavaCodeVisionSettings>
          <option name="showImplementations" value="true" />
          <option name="showUsages" value="true" />
        </JavaCodeVisionSettings>
      </JAVA.JavaLens>
      <kotlin.KotlinReferencesTypeHintsProvider>
        <Settings>
          <option name="functionReturnType" value="true" />
          <option name="localVariableType" value="true" />
          <option name="parameterType" value="true" />
          <option name="propertyType" value="true" />
        </Settings>
      </kotlin.KotlinReferencesTypeHintsProvider>
    </settingsMapElement>
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>